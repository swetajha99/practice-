import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        // write ur code here
        int N = scn.nextInt();
        int k=1;
        for(int i=1;i<=N;i++){
            for(int j=1;j<=i;j++){
                System.out.print(k++ +"\t");
            }
            System.out.println();
        }
        

    }
}


